<Window x:Class="POOmall.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POOmall"
        mc:Ignorable="d"
        Height="256"
        Width="300"
        ResizeMode="NoResize">
  <StackPanel Margin="15">
    <Label Content="Dinero Inicial"></Label>
    <TextBox Name="dinero"/>
    <Label Content="Sueldo Promedio"></Label>
    <TextBox Name="sueldo"/>
    <Label Content="Precio m²"></Label>
    <TextBox Name="preciom2"/>
    <Button Name="BtnListo" Margin="0,25,0,0" Height="30" Content="Listo" Click="BtnListo_OnClick"/>
  </StackPanel>
</Window>
